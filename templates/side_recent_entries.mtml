<mt:If name="main_index">
<mt:If tag="BlogEntryCount">
<mt:Entries limit="5" sort_by="modified_on">
<mt:EntriesHeader>
<div class="sidebar-module">
<h4>最近更新の記事</h4>

<ol class="list-unstyled">
</mt:EntriesHeader>
<li><a href="<$mt:EntryPermalink$>"><$mt:EntryTitle$></a></li>
<mt:EntriesFooter>
</ol>

</div>
</mt:EntriesFooter>
</mt:Entries>
</mt:If>
</mt:If>
